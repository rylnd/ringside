    "@types/node": "^10.12.15",
    "browserify": "^16.2.3",
    "clean-webpack-plugin": "^1.0.0",
    "d3-scale-chromatic": "^1.3.3",
    "jest": "^23.6.0",
    "prettier": "^1.15.3",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "standard-version": "^4.4.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-loader": "^3.5.4",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.2.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "1.7.2"
